//Binary to decimal conversion
#include<stdio.h>
#include<math.h>
int main(){
    int num;
    printf("Enter number:");
    scanf("%d",&num);
    int num1=num;
    int sum=0,p=0,rem;
    while(num!=0){
        rem=num%10;
        sum+=rem*pow(2,p);
        p++;
        num=num/10;
    }
    printf("Binary of %d is %d",num,sum);
    return 0;
}
