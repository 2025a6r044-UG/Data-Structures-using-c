//Program to convert octal into decimal to binary number
#include<stdio.h>
int OctalToDecimal(int num){
    int decnum=0,base=1;
    while(num!=0){
        int rem=num%10;
        decnum+=rem*base;
        base*=8;
        num/=10;
    }
    return decnum;
}
int DecimalToBinary(int num){
    int bin=0,place=1;
    while(num!=0){
        int rem=num%2;
        bin=bin+rem*place;
        num=num/2;
        place=place*10;

    }
    return bin;
}
int main(){
    int num;
    printf("Enter octal number:");
    scanf("%d",&num);
    int binary=DecimalToBinary(OctalToDecimal(num));
    printf("Binary of Octal number %d is %d",num,binary);
    return 0;
}
